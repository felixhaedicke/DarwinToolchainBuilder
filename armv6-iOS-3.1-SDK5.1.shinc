DARWIN_SDK_VERSION=5.1
IOS_MIN_SUPPORTED_VERSION=3.1
TARGET_ARCH=armv6
TARGET_TRIPLE=${TARGET_ARCH}-apple-darwin9
TARGET_TYPE=ios
PREFIX=${DARWIN_TOOLCHAIN}/lib/${TARGET_ARCH}-iOS-${IOS_MIN_SUPPORTED_VERSION}-SDK${DARWIN_SDK_VERSION}.sdk
BUILD_ZLIB=1
BUILD_LIBCXX=1

CFLAGS="-O2 -miphoneos-version-min=${IOS_MIN_SUPPORTED_VERSION}"
CXXFLAGS=${CFLAGS}

